{
    "name": "corianderphp/corianderphp",
    "description": "A lightweight, performance-focused PHP framework.",
    "type": "project",
    "keywords": ["php", "framework", "eco-design", "performance"],
    "homepage": "https://github.com/CorianderPHP/CorianderPHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Lohan Dancuo",
            "homepage": "https://lohan.dancuo.fr/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "nyholm/psr7": "^1.8",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
    "autoload": {
        "psr-4": {
            "CorianderCore\\Core\\": "CorianderCore/core/",
            "CorianderCore\\Modules\\": "CorianderCore/modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CorianderCore\\Tests\\": "CorianderCore/tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
